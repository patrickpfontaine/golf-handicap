{
  "expo": {
    "name": "Golf Handicap",
    "slug": "golf-handicap",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.patrickfontaine.golfhandicap"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.patrickfontaine.golfhandicap"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "984245b1-fc44-486f-bf8c-d255dce601fa"
      }
    },
    "owner": "patrickfontaine",
    "updates": {
      "url": "https://u.expo.dev/984245b1-fc44-486f-bf8c-d255dce601fa"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
